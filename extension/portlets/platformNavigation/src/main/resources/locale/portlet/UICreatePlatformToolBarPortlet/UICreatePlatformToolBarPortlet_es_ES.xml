<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Debate</topic>
         <poll>Encuesta</poll>
         <event>Evento</event>
         <upload>Archivo</upload>
         <wiki>Página Wiki</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>un debate</topic>
         <poll>Encuesta</poll>
         <CreateANew>Crear {1}:</CreateANew>
         <NoForum>Lo sentimos, aún no hay ningún foro disponible para crear una {1}. Primero empiece creando un espacio y vuelva a intentarlo.</NoForum>
         <RequireSelectForum>Por favor seleccione un foro.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Seleccione un foro</SelectAForum>
         <location>En lugar</location>
         <forumId>Y el foro</forumId>
         <FilterForums>Filtrar Foros</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Seleccionar foro</SelectAForum>
         <location>En lugar</location>
         <forumId>Y el foro</forumId>
         <FilterForums>Filtrar Foros</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>Crear una página Wiki:</title>
         <location>Ubicación</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Crear un nuevo Evento</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Añadir nuevo:</Choix>
         <EndEvent>A</EndEvent>
         <StartEvent>De</StartEvent>
         <Calendar>Calendario</Calendar>
         <all-day>Todo el día</all-day>
         <defaultCalendarId>Por Defecto</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Se requiere el campo "Título".</summary-field-required>
         <fromDate-format>El formato de fecha "Desde" es incorrecto.</fromDate-format>
         <toDate-format>El formato de fecha "Hasta" es incorrecto.</toDate-format>
         <logic-required>Lo sentimos, no puede crear un evento que termina antes de que empiece.</logic-required>
         <cant-add-event-on-remote-calendar>No puede añadir eventos al calendario remoto</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>El evento ha sido añadido a {1}</Event>
            <Task>La tarea ha sido añadida a {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} no fue añadido.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Seleccionar archivo</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Evento</Event>
         <Task>Tarea</Task>
      </label>
   </Choix>
</bundle>
