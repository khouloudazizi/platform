<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Sondaj</poll>
         <event>Event</event>
         <upload>File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>New {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Vă rugăm să selectaţi un forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Selectaţi un Forum</SelectAForum>
         <location>Locaţie</location>
         <forumId>Şi Forum</forumId>
         <FilterForums>Filtru forumuri</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Selectaţi Forum</SelectAForum>
         <location>Locaţie</location>
         <forumId>Şi Forum</forumId>
         <FilterForums>Filtru forumuri</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event :</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Adăugă:</Choix>
         <EndEvent>Până la</EndEvent>
         <StartEvent>De la</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>Toată ziua</all-day>
         <defaultCalendarId>Default</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Câmpul "Titlul" este necesar.</summary-field-required>
         <fromDate-format>Formatul câmpului "De la" este incorect.</fromDate-format>
         <toDate-format>Formatul câmpului "data la" este incorect.</toDate-format>
         <logic-required>Ne pare rau, nu puteţi crea un eveniment care se termină înainte de a începe.</logic-required>
         <cant-add-event-on-remote-calendar>Nu puteți adăuga un eveniment la calendare externe</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>Evenimentul a fost adăugat la {1}</Event>
            <Task>Activitatea a fost adăugată la {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} nu a fost adăugat.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Selectaţi fişierul</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Eveniment</Event>
         <Task>Activitate</Task>
      </label>
   </Choix>
</bundle>
