<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Anketa</poll>
         <event>Event</event>
         <upload>File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>New {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Vyberte prosím fórum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Vybrat fórum</SelectAForum>
         <location>V umístění</location>
         <forumId>A fórum</forumId>
         <FilterForums>Filtrovat fóra</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Vybrat fórum</SelectAForum>
         <location>V umístění</location>
         <forumId>A fórum</forumId>
         <FilterForums>Filtrovat fóra</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Přidat novou:</Choix>
         <EndEvent>Do</EndEvent>
         <StartEvent>Od</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>Celý den</all-day>
         <defaultCalendarId>Výchozí</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Pole "Nadpis" je vyžadováno.</summary-field-required>
         <fromDate-format>Formát data "Od" je nesprávný.</fromDate-format>
         <toDate-format>Formát data "Do" je nesprávný.</toDate-format>
         <logic-required>Lituji, nemůžete vytvořit událost, která končí dříve, než začne.</logic-required>
         <cant-add-event-on-remote-calendar>Nelze přidat událost do vzdáleného kalendáře</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>Událost byla přidána do {1}</Event>
            <Task>Úkol byl přidán do {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} nebyla přidána.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Vybrat soubor</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Událost</Event>
         <Task>Úkol</Task>
      </label>
   </Choix>
</bundle>
