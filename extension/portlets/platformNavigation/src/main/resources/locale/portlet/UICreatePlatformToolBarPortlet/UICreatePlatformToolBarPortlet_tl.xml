<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Poll</poll>
         <event>Ang Kaganapan</event>
         <upload>Ang File</upload>
         <wiki>Ang Pahina ng Wiki</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>ang talakayan</topic>
         <poll>Ang Poll</poll>
         <CreateANew>Ang Bagong {1}:</CreateANew>
         <NoForum>Paumanhin, walang forum na magagamit pa kung saan maaari kang lumikaha ng {1}. Magsimula sa paglikha ng espasyo muna at pagkatapos ay subukan muli.</NoForum>
         <RequireSelectForum>Please select a forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Pumili ng isang Forum</SelectAForum>
         <location>Sa Lokasyon</location>
         <forumId>At ang Forum</forumId>
         <FilterForums>I-filter ang Mga Forum</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Piliin ang Forum</SelectAForum>
         <location>Sa Lokasyon</location>
         <forumId>At ang Forum</forumId>
         <FilterForums>I-filter ang Mga Forum</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>Isang Bagong Pahina ng Wiki:</title>
         <location>Ang Lokasyon</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Add New:</Choix>
         <EndEvent>Para sa</EndEvent>
         <StartEvent>Mula sa</StartEvent>
         <Calendar>Ang Kalendaryo</Calendar>
         <all-day>All Day</all-day>
         <defaultCalendarId>Default</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>The field "Title" is required.</summary-field-required>
         <fromDate-format>The "From" date format is incorrect.</fromDate-format>
         <toDate-format>The "To" date format is incorrect.</toDate-format>
         <logic-required>Sorry, you cannot create an event that ends before it starts.</logic-required>
         <cant-add-event-on-remote-calendar>You cannot add event to remote calendar</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>The Event was added to {1}</Event>
            <Task>The Task was added to {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} was not added.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Select File</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Ang Kaganapan</Event>
         <Task>Task</Task>
      </label>
   </Choix>
</bundle>
