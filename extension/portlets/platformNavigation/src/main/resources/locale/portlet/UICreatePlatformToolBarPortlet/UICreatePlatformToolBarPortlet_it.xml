<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Sondaggio</poll>
         <event>Evento</event>
         <upload>File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>New {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Prego scegliere il forum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Selezionare un Forum</SelectAForum>
         <location>Dentro di</location>
         <forumId>E Forum</forumId>
         <FilterForums>Filtrare i Forum</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Selezionare Forum</SelectAForum>
         <location>Dentro di</location>
         <forumId>E Forum</forumId>
         <FilterForums>Filtrare i Forum</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event :</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Aggiungere nuovo:</Choix>
         <EndEvent>A</EndEvent>
         <StartEvent>Da</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>Tutto il giorno</all-day>
         <defaultCalendarId>Predefinito</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Il campo "Titolo" è obbligatorio.</summary-field-required>
         <fromDate-format>Il formato della data "Da" non è corretto.</fromDate-format>
         <toDate-format>Il formato della data "A" non è corretto.</toDate-format>
         <logic-required>Siamo spiacenti, non è possibile creare un evento che termina prima che inizi.</logic-required>
         <cant-add-event-on-remote-calendar>Non è possibile aggiungere eventi al calendario remoto</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>L'evento è stato aggiunto a {1}</Event>
            <Task>Il Compito è stato aggiunto a {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} non è stato aggiunto.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Selezionare File</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Evento</Event>
         <Task>Compito</Task>
      </label>
   </Choix>
</bundle>
