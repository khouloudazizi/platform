<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Enquesta</poll>
         <event>Esdeveniment</event>
         <upload>Fitxer</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>New {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Si us plau, selecciona un fòrum.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Selecciona un fòrum</SelectAForum>
         <location>Ubicació</location>
         <forumId>I Fòrum</forumId>
         <FilterForums>Filtra Fòrums</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Selecciona un Fòrum</SelectAForum>
         <location>Ubicació</location>
         <forumId>I Fòrum</forumId>
         <FilterForums>Filtra Fòrums</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event :</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Afegeix un nou:</Choix>
         <EndEvent>A</EndEvent>
         <StartEvent>Des de</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>Tot el dia</all-day>
         <defaultCalendarId>Per defecte</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>El camp "Títol" s'exigeix.</summary-field-required>
         <fromDate-format>El format de data "Des de" és incorrecta.</fromDate-format>
         <toDate-format>La data inicial té un format incorrecte.</toDate-format>
         <logic-required>Ho sentim, no pot crear un esdeveniment que acaba abans que comença.</logic-required>
         <cant-add-event-on-remote-calendar>No pot afegir esdeveniments al calendari remot</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>L'esdeveniment es va afegir a {1}</Event>
            <Task>La tasca es va afegir a {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} no s'ha afegit.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Selecciona el Fitxer</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Esdeveniment</Event>
         <Task>Tasca</Task>
      </label>
   </Choix>
</bundle>
