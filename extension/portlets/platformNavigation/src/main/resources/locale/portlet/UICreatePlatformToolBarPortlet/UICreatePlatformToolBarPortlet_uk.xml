<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Опитування</poll>
         <event>Подія</event>
         <upload>File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>New {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Будь ласка, виберіть форум.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Виберіть форум</SelectAForum>
         <location>У місці</location>
         <forumId>І форум</forumId>
         <FilterForums>Фільтрувати форуми</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Вибрати форум</SelectAForum>
         <location>У місці</location>
         <forumId>І форум</forumId>
         <FilterForums>Фільтрувати форуми</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event :</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Додати новий:</Choix>
         <EndEvent>До</EndEvent>
         <StartEvent>Від</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>Весь день</all-day>
         <defaultCalendarId>За замовчуванням</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>Поле "Назва" є обов'язковим.</summary-field-required>
         <fromDate-format>Невірний формат дати "Від".</fromDate-format>
         <toDate-format>Невірний формат дати "До".</toDate-format>
         <logic-required>На жаль, не можна створити подію, яка закінчується до її початку.</logic-required>
         <cant-add-event-on-remote-calendar>Не можна додати подію у віддалений календар</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>Подія була додана до {1}</Event>
            <Task>Завдання було додано до {1}</Task>
         </add-successfully>
         <add-unsuccessfully>{0} не було додано.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Вибрати файл</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Подія</Event>
         <Task>Завдання</Task>
      </label>
   </Choix>
</bundle>
