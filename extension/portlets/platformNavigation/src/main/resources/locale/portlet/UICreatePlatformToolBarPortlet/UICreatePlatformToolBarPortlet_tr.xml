<?xml version="1.0" encoding="UTF-8"?>
<bundle>
   <UIUserPlatformToolBarCreatePortlet>
      <label>
         <topic>Discussion</topic>
         <poll>Anket</poll>
         <event>Etkinlik</event>
         <upload>File</upload>
         <wiki>Wiki Page</wiki>

      </label>
   </UIUserPlatformToolBarCreatePortlet>
   <UICreateList>
      <label>
         <topic>discussion</topic>
         <poll>Poll</poll>
         <CreateANew>New {1}:</CreateANew>
         <NoForum>Sorry, no forum is available yet in which you can create the {1}. Start by creating a space first and then try again.</NoForum>
         <RequireSelectForum>Lütfen bir forum seçin.</RequireSelectForum>
      </label>
      <action>
         <Next>#{word.next}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateList>
   <UICreateTopic>
      <label>
         <SelectAForum>Bir Forum seçin</SelectAForum>
         <location>Konum</location>
         <forumId>Ve Forum</forumId>
         <FilterForums>Forumları Süz</FilterForums>
      </label>
   </UICreateTopic>
   <UICreatePoll>
      <label>
         <SelectAForum>Forum Seçin</SelectAForum>
         <location>Konum</location>
         <forumId>Ve Forum</forumId>
         <FilterForums>Forumları Süz</FilterForums>
      </label>
   </UICreatePoll>
   <UICreateWiki>
      <label>
         <title>New Wiki Page:</title>
         <location>Location</location>
      </label>
   </UICreateWiki>
   <UICreateEvent>
      <label>
         <Popup_title>Create a new Event</Popup_title>
         <Title>#{word.title}</Title>
         <Choix>Yeni ekle:</Choix>
         <EndEvent>Bitiş Tarihi</EndEvent>
         <StartEvent>Başlangıç Tarihi</StartEvent>
         <Calendar>Calendar</Calendar>
         <all-day>Tüm Gün</all-day>
         <defaultCalendarId>Varsayılan</defaultCalendarId>
      </label>
      <msg>
         <summary-field-required>"Başlık" alanı gereklidir.</summary-field-required>
         <fromDate-format>"Başlangıç" tarihi biçimi doğru değil.</fromDate-format>
         <toDate-format>"Bitiş" tarihi biçimi doğru değil.</toDate-format>
         <logic-required>Üzgünüz, başlamadan biten bir etkinlik oluşturulamıyor.</logic-required>
         <cant-add-event-on-remote-calendar>Uzaktan takvime olay eklenemiyor</cant-add-event-on-remote-calendar>
         <add-successfully>
            <Event>{1} için Etkinlik  eklendi</Event>
            <Task>{1} için Görev eklendi</Task>
         </add-successfully>
         <add-unsuccessfully>{0} eklenemedi.</add-unsuccessfully>
      </msg>
      <action>
         <Next>#{word.save}</Next>
         <Cancel>#{word.cancel}</Cancel>
      </action>
   </UICreateEvent>
    <UIUploadFile>
        <Select>Dosya Seçin</Select>
        <action>
          <close>#{word.close}</close>
        </action>
    </UIUploadFile>
   <Choix>
      <label>
         <Event>Etkinlik</Event>
         <Task>Görev</Task>
      </label>
   </Choix>
</bundle>
